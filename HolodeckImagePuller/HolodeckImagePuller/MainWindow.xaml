<Window x:Class="WpfApplication1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Holotable Image Puller" Height="350" Width="525">
    <Grid>
        <Button Content="Get Images!" Height="23" HorizontalAlignment="Left" Margin="428,117,0,0" Name="myGoButton" VerticalAlignment="Top" Width="75" Click="myGoButton_Click" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="259,30,0,0" Name="myHolotableFolder" VerticalAlignment="Top" Width="244" Text="c:\Holotable" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="12,33,0,0" Name="textBlock1" Text="Enter path to Holotable\cards\starwars:" VerticalAlignment="Top" Width="226" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="12,76,0,0" Name="textBox2" Text="Enter path to hold the offline-vkit cards:" VerticalAlignment="Top" Width="226" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="259,73,0,0" Name="myOutputFolder" VerticalAlignment="Top" Width="244" Text="c:\vcards" />
    </Grid>
</Window>
